pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
            withMaven(maven : 'maven3.3.9'){
              sh 'mvn clean compile'
                echo 'Compiling.'
                }
            }
        }
       stage('Testing') {
            steps {
            withMaven(maven : 'maven3.3.9'){
              sh 'mvn test'
                echo 'Testing..'
                }
            }
        }
       stage('Deploy') {
            steps {
            withMaven(maven : 'maven3.3.9'){
              sh 'mvn deploy'
                echo 'Building..'
                }
            }
        }
    }
}
